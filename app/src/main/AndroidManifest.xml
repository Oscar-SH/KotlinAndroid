<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.isesion">

    <uses-permission android:name="android.permission.CALL_PHONE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Isesion">
        <activity
            android:name=".AcercaDe"
            android:label="Acerca De"
            android:exported="false" />
        <activity
            android:name=".Avisos"
            android:label="Avisos"
            android:exported="false" />
        <activity
            android:name=".Asignaturas"
            android:label="Asignaturas"
            android:exported="false" />
        <activity
            android:name=".Institucion"
            android:label="Institucion"
            android:exported="false" />
        <activity
            android:name=".Datos"
            android:label="Datos"
            android:exported="false" />
        <activity
            android:name=".Home"
            android:label="Bienvenido"
            android:exported="false"
            android:parentActivityName=".sesion">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".sesion" />
        </activity>
        <activity
            android:name=".sesion"
            android:label="Iniciar Sesion"
            android:exported="false"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="Inicio"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>